alias x='exit'
alias cls='clear'
alias r='source ~/.zshrc'

alias ff='fastfetch'

alias ls='eza --icons'
alias la='eza --icons -A'
alias lt='eza --icons -T'

alias ydmp4='yt-dlp -f bestvideo+bestaudio --merge-output-format mp4'
alias ydmkv='yt-dlp -f bestvideo+bestaudio --merge-output-format mkv'
alias ydmp3='yt-dlp -f bestaudio --extract-audio --audio-format mp3 --audio-quality 0'
alias ydflac='yt-dlp -f bestaudio --extract-audio --audio-format flac --audio-quality 0'
